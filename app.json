{
  "expo": {
    "name": "Picza",
    "slug": "picza",
    "version": "1.0.6",
    "scheme": "picza",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "io.picza.app",
      "buildNumber": "9",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": ["applinks:picza.app"]
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "18e3f17b-3900-4386-a72c-2d26b071afc6"
      }
    }
  }
}
